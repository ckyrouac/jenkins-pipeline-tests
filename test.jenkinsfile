node {
  stage("test") {
    checkout(scm)
    sh("git checkout master")
    sh("git fetch origin && git reset --hard origin/master")
    HEAD = sh (
        script: "git rev-parse HEAD",
        returnStdout: true
    ).trim()
    echo "HEAD: ${HEAD}"
    
    PREVIOUS_BUILD = currentBuild.getPreviousBuild().result
    echo "PREVIOUS_BUILD: ${PREVIOUS_BUILD}"
  }
}
